{
  "name": "design-troopers",
  "version": "1.4.0",
  "main": "dist/main.js",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "scripts": {
    "gen:tokens": "node scripts/generateTokens.js > tokens.json",
    "gen:tokens:dev": "nodemon --exec 'yarn gen:tokens' --watch src/tokens",
    "gen:iconslist": "node scripts/generateIconsList.js > iconsList.json",
    "copy:tokens": "cp tokens.json dist/tokens.json",
    "docs": "styleguidist server",
    "docs:dev": "nodemon --exec 'yarn docs' --watch styleguide.config.js --watch package.json",
    "docs:build": "styleguidist build",
    "docs:deploy": "yarn docs:build && surge -p styleguide -d https://design-troopers.surge.sh",
    "build": "webpack --mode production",
    "test": "jest",
    "test:dev": "yarn test --watch",
    "test:debug": "node --inspect-brk $(npm bin)/jest --runInBand --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run --reporter cypress-image-snapshot/reporter",
    "prepublishOnly": "yarn build && yarn gen:tokens && yarn copy:tokens",
    "postpublish": "yarn docs:deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.1.0",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "cypress-image-snapshot": "^3.1.1",
    "flow-bin": "^0.105.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "packagebind": "^0.3.3",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-docgen-external-proptypes-handler": "^1.0.3",
    "react-docgen-imported-proptype-handler": "^1.0.4",
    "react-dom": "^16.9.0",
    "react-styleguidist": "^9.1.14",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "material-svg-react-icons": "3",
    "tailwindcss": "^1.1.0",
    "tailwindcss-transitions": "^2.0.1"
  }
}
